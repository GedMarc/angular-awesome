    "docs:deploy": "npm run docs:build && npx gh-pages -d dist/angular-awesome"
  },
  "main": "dist/angular-awesome/fesm2022/angular-awesome.js",
  "module": "dist/angular-awesome/fesm2022/angular-awesome.js",
